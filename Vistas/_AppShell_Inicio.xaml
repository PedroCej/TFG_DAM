<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProyectoTFG.Vistas._AppShell_Inicio"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProyectoTFG"
    xmlns:misPaginas="clr-namespace:ProyectoTFG.Vistas"
    Title="ProyectoTFG"
    FlyoutBackgroundColor="#000000f3"
    Shell.FlyoutBehavior="Locked"
    Shell.FlyoutWidth="200">


    <FlyoutItem
        Title="Inicio"
        FlyoutIcon="home.png"
        Icon="home.png">
        <ShellContent ContentTemplate="{DataTemplate misPaginas:Inicio}" Route="home" />
    </FlyoutItem>
    <FlyoutItem Title="Tickets" Icon="warning.png">
        <ShellContent ContentTemplate="{DataTemplate misPaginas:Inicio_Tickets}" />
    </FlyoutItem>
    <FlyoutItem Title="Técnico" Icon="tecnico.png">
        <ShellContent ContentTemplate="{DataTemplate misPaginas:Inicio_Tecnico}" />
    </FlyoutItem>
    <FlyoutItem Title="Administración" Icon="admin.png">
        <ShellContent ContentTemplate="{DataTemplate misPaginas:Inicio_Admin}" />
    </FlyoutItem>
    <FlyoutItem Title="Ajustes" Icon="settings.png">
        <ShellContent ContentTemplate="{DataTemplate misPaginas:Inicio_Ajustes}" />
    </FlyoutItem>
    <FlyoutItem Title="Salir" Icon="salir.png">
        <ShellContent ContentTemplate="{DataTemplate misPaginas:Inicio_Salir}" />
    </FlyoutItem>



</Shell>
